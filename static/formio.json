{
  "display": "form",
  "components": [
    {
      "label": "Merchandises To Transact",
      "tableView": false,
      "rowDrafts": false,
      "key": "merchandisesToTransact",
      "type": "editgrid",
      "displayAsTable": false,
      "input": true,
      "components": [
        {
          "label": "Columns",
          "columns": [
            {
              "components": [
                {
                  "label": "Artist ID",
                  "widget": "choicesjs",
                  "placeholder": "-- Select Artist ID --",
                  "tableView": true,
                  "dataSrc": "url",
                  "data": {
                    "url": "http://0.0.0.0:5000/user/artistIds",
                    "headers": [
                      {
                        "key": "",
                        "value": ""
                      }
                    ]
                  },
                  "key": "artistId",
                  "type": "select",
                  "selectValues": "data",
                  "hideOnChildrenHidden": false,
                  "disableLimit": false,
                  "input": true
                }
              ],
              "width": 6,
              "offset": 0,
              "push": 0,
              "pull": 0,
              "size": "md",
              "currentWidth": 6
            },
            
            {
              "components": [
                {
                  "label": "Merch ID",
                  "widget": "choicesjs",
                  "tableView": true,
                  "dataSrc": "url",
                  "data": {
                    "url": "http://0.0.0.0:5000/user/{{data.curr.artistId}}/merch/id",
                    "headers": [
                      {
                        "key": "",
                        "value": ""
                      }
                    ]
                  },
                  "template":"<span>{{ item.label}}<img src=\"https://drive.google.com/uc?export=preview&id=12K98sWK_zbGf-MfdbaXdNhvdOwMvNtRk\" width=\"32\"></img></span>",
                  "refreshOn": "artistId",
                  "key": "merchId",
                  "type": "select",
                  "selectValues": "data",
                  "disableLimit": false,
                  "input": true
                }
              ],
              "width": 6,
              "offset": 0,
              "push": 0,
              "pull": 0,
              "size": "md",
              "currentWidth": 6
            },
            
            {
              "components": [
                {
                  "label": "Price ID",
                  "widget": "choicesjs",
                  "tableView": true,
                  "dataSrc": "url",
                  "data": {
                    "url": "http://0.0.0.0:5000/user/{{data.curr.artistId}}/merch/{{data.curr.merchId}}/price",
                    "headers": [
                      {
                        "key": "",
                        "value": ""
                      }
                    ]
                  },
                  "template":"<span>{{ item.label}}</span>",
                  "refreshOn": "merchId",
                  "key": "price",
                  "type": "select",
                  "selectValues": "data",
                  "disableLimit": false,
                  "input": true
                }
              ],
              "width": 6,
              "offset": 0,
              "push": 0,
              "pull": 0,
              "size": "md",
              "currentWidth": 6
            },
            {
              "components": [
                {
                  "label": "Quantity",
                  "widget": "choicesjs",
                  "tableView": true,
                  "dataSrc": "url",
                  "data": {
                    "url": "http://0.0.0.0:5000/user/{{data.curr.artistId}}/merch/{{data.curr.merchId}}/qty/range",
                    "headers": [
                      {
                        "key": "",
                        "value": ""
                      }
                    ]
                  },
                  "template":"<span>{{ item.label}}</span>",
                  "refreshOn": "merchId",
                  "key": "qty",
                  "type": "select",
                  "selectValues": "data",
                  "disableLimit": false,
                  "input": true
                }
              ],
              "width": 6,
              "offset": 0,
              "push": 0,
              "pull": 0,
              "size": "md",
              "currentWidth": 6
            }
          ],
          "key": "columns",
          "type": "columns",
          "input": false,
          "tableView": false
        }
      ]
    },
    {
      "type": "button",
      "label": "Submit",
      "key": "submit",
      "disableOnInvalid": true,
      "input": true,
      "tableView": false
    }
  ]
}
