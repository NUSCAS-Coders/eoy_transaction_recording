{
  "display": "form",
  "components": [
    {
      "label": "Merchandises To Transact",
      "tableView": false,
      "rowDrafts": false,
      "key": "merchandisesToTransact",
      "type": "editgrid",
      "displayAsTable": false,
      "input": true,
      "components": [
        {
          "label": "Columns",
          "columns": [
            {
              "components": [
                {
                  "label": "Artist ID",
                  "widget": "choicesjs",
                  "placeholder": "i.e A",
                  "tableView": true,
                  "dataSrc": "url",
                  "data": {
                    "url": "http://<SERVER_URL>:5000/user/artistIds",
                    "headers": [
                      {
                        "key": "",
                        "value": ""
                      }
                    ]
                  },
                  "key": "artistId",
                  "type": "select",
                  "selectValues": "data",
                  "hideOnChildrenHidden": false,
                  "disableLimit": false,
                  "input": true
                }
              ],
              "width": 6,
              "offset": 0,
              "push": 0,
              "pull": 0,
              "size": "md",
              "currentWidth": 6
            },
            
            {
              "components": [
                {
                  "label": "Merch ID",
                  "placeholder": "i.e A5 (KC)",
                  "widget": "choicesjs",
                  "tableView": true,
                  "dataSrc": "url",
                  "data": {
                    "url": "http://<SERVER_URL>:5000/user/{{data.artistId}}/merch/id",
                    "headers": [
                      {
                        "key": "",
                        "value": ""
                      }
                    ]
                  },
                  "template":"<span>{{ item.label}} {{item.embedCode}}</span>",
                  "refreshOn": "artistId",
                  "key": "merchId",
                  "type": "select",
                  "selectValues": "data",
                  "disableLimit": false,
                  "input": true
                }
              ],
              "width": 6,
              "offset": 0,
              "push": 0,
              "pull": 0,
              "size": "md",
              "currentWidth": 6
            },
            
            {
              "components": [
                {
                  "label": "Price",
                  "placeholder": "i.e $5",
                  "widget": "choicesjs",
                  "tableView": true,
                  "dataSrc": "url",
                  "data": {
                    "url": "http://<SERVER_URL>:5000/user/{{data.artistId}}/merch/{{data.merchId}}/price",
                    "headers": [
                      {
                        "key": "",
                        "value": ""
                      }
                    ]
                  },
                  "template":"<span>{{ item.label}}</span>",
                  "key": "price",
                  "refreshOn": "merchId",
                  "type": "select",
                  "selectValues": "data",
                  "disableLimit": false,
                  "input": true
                }
              ],
              "width": 6,
              "offset": 0,
              "push": 0,
              "pull": 0,
              "size": "md",
              "currentWidth": 6
            },
            {
              "components": [
                {
                  "label": "Quantity",
                  "placeholder": "i.e 5",
                  "widget": "choicesjs",
                  "tableView": true,
                  "dataSrc": "url",
                  "data": {
                    "url": "http://<SERVER_URL>:5000/user/{{data.artistId}}/merch/{{data.merchId}}/qty/range",
                    "headers": [
                      {
                        "key": "",
                        "value": ""
                      }
                    ]
                  },
                  "template":"<span>{{ item.label}}</span>",
                  "refreshOn": "merchId",
                  "key": "qty",
                  "type": "select",
                  "selectValues": "data",
                  "disableLimit": false,
                  "input": true
                }
              ],
              "width": 6,
              "offset": 0,
              "push": 0,
              "pull": 0,
              "size": "md",
              "currentWidth": 6
            },

            {
              "components": [
                {
                "label": "Datetime",
                "disabled": true,
                "tableView": true,
                "key": "datetime",
                "type": "textfield",
                "input": true
                }
              ],
              "width": 6,
              "offset": 0,
              "push": 0,
              "pull": 0,
              "size": "md",
              "currentWidth": 6
            }
          ],
          "key": "columns",
          "type": "columns",
          "input": false,
          "tableView": false
        }

      ]
    },
    {
      "type": "button",
      "label": "Submit",
      "key": "submit",
      "disableOnInvalid": true,
      "input": true,
      "tableView": false
    }
  ]
}
